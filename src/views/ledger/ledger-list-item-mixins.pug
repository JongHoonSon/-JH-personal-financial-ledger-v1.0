mixin list(item)
    li(class=`${item.type === "i" ? 'incomeBgColor' : 'expenseBgColor'}`) 
        a(href=`/item/detail/${item.type}/${item.id}`).ledger-list-item-mixins
            ul
                li #{'날짜 : ' + item.stringDate}
                li #{'금액 : ' + item.amount + '원'}
                li #{'분류 : ' + item.category}